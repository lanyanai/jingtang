/* CSS Document */
/*footer*/
.mod-nav-onion{height: 48px;;line-height: 48px;background-color: #f49643; clear:both;border-top: 1px solid #ebe7e3;margin-top: 10px;}
#subTab ~ .mod-nav-onion{border-top: none;}
.mod-nav-onion>ul{
	float: left;	
	white-space:nowrap;
	overflow:hidden;
	text-overflow: ellipsis;
}
.mod-nav-onion>ul li{display: inline-block;min-width: 48px;padding:0 16px 0 12px;line-height: 48px;background: url("data:image/png;base64,") no-repeat right center;-webkit-background-size: auto 46px;background-size: auto 46px;text-align: center;font-size: 14px;}
.mod-nav-onion>ul li a{display: block;color: #fff;padding-right: 5px; font-weight: bolder;}
.mod-nav-onion>ul li a:visited{color:#fff;}
.mod-nav-onion>ul li:last-child{background: none;}
.mod-nav-onion>ul li:last-child a{color: #fff}
.mod-nav-onion>ul li:last-child a:visited{color:#fff;}
.mod-nav-onion .to-top{float: right;width: 66px;}
.mod-nav-onion>ul + .to-top {border-left: 1px solid #f7f3f3;}
.mod-nav-onion>ul + .to-top span{border-left:1px solid #d6d6d6;}
.mod-nav-onion .to-top span{display: block;text-indent: -9999em;overflow: hidden;background: url("../images/local/top.png") no-repeat center center;-webkit-background-size:12px auto;background-size:12px auto; }

.baidu{
	float: left;
	margin-left: 10px;
	margin-top: 4px;
}