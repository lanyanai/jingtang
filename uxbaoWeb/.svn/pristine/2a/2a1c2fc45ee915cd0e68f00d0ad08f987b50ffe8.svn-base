body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;-webkit-tap-highlight-color: rgba(0,0,0,0);}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
    color:#252525; font-size:14px; line-height:20px; -webkit-user-select:none;
}
.logo{ margin:5px 15px; background: url("../images/login/tit.png") left center no-repeat;
    border-bottom: 1px #d3d9d5 dashed; height: 60px; }
#login_form,#register_form{margin:15px;}

.err-holder{
    line-height: 22px;
    color: #ea3106;
    font-size: 16px;
    margin-bottom:5px;
}

.login-frame li, .register-frame li{
    margin-bottom: 15px;
    position: relative;
}
.login-frame .input-box, .register-frame .input-box{
    border: 1px solid #adadad;
    background: #f9f9f9;
    border-radius: 6px;
    height: 30px;
    padding:10px;
    color: #828282;
    font-size: 16px;
}
.login-frame .active, .register-frame .active
{
    border-color: #ee8020;
}

.login-frame .default, .register-frame .default{
    border: none;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    width: 180px;
    color: #323232;
    background: #f9f9f9;
    font-size: 16px;
    line-height: 30px;
}
input::-webkit-input-placeholder
{
    line-height: 30px;
}
textarea, input {
    resize: none;
    outline: none;
    -webkit-tap-highlight-color: rgba(255,0,0,0);
}
.icon-btn {
    display: block;
    height: 50px;
    line-height: 50px;
    text-align: center;
    color: #fff;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    width: 100%;
    font-size: 16px;
    font-weight: bold;
    background: url(../images/login/btn_bg.png);
}

.link{ float:right; color: #e97c20; margin:10px; font-weight: bold; font-size: 16px;}