/* CSS Document */
body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family:'微软雅黑'; color:#252525; font-size:14px; line-height:20px;}

.more{width:60%; height:30px; background:#edf5e1; border-radius:5px; margin:10px auto; line-height:30px; color:#669900; text-align:center;}
.more a{display:block; width:100%; height:30px; text-align:center; line-height:30px; color:#669900; font-size:16px;}

.scroll{width:100%; height:auto; margin:0 auto;  position:relative; overflow:hidden;}
.mod_01{float:left;}
.mod_01 img{display:block; width:100%; height:auto;}
.dotModule_new {padding:0;height:20px;line-height:20px;-webkit-border-radius:11px;position:absolute;bottom:5px;right:10%;z-index:11;width:80%;}
#slide_01_dot {text-align:center;}
#slide_01_dot span {display:inline-block;margin:0 6px;width:25px;height:10px;vertical-align:middle;background:#c6c6c6;-webkit-border-radius:5px;}
#slide_01_dot .selected {background:#f68928;}

.type{overflow: hidden; margin:10px;}
.type ul{width:100%;}
.type ul li{width: 20%; float: left;}
.type ul li a,.type ul li a span{color: #636363;display: block;}
.type ul li a img {border-radius: 6px;height: auto;width:60px; display: block; margin:0 auto;}
.type ul li a span {height: 24px;line-height: 24px;padding: 2px 0;width: 100%; text-align: center;}

.appList{margin:10px 3px;}
#app-list-box{width:100%; overflow: hidden;}
#app-list-box li{width:50%;float:left;margin:5px auto;}
.itemAll{border:1px solid #e7e7e7;border-radius:6px;margin:0 3px;padding: 5px;overflow: hidden;}
.appInfo{width:80%;float: left;}
.appInfo a{display: block; width: 100%;}
.appIcon{width:60px; height: 60px; display: block; float: left;}
.tit{float:left;}
.tit strong{padding: 3px 5px;font-size: 18px;color: #858484;line-height: 24px;}
.tit .items-score{padding: 9px 5px;}
.btn{display: block;float: left;width: 20%;height: 60px;}
.dlBtn{background: url("../images/jingpin/down.png") top center no-repeat;}
.cancelBtn{background: url("../images/jingpin/close.png") top center no-repeat;}
.openBtn{background: url("../images/jingpin/down.png") top center no-repeat;}
.updateBtn{background: url("../images/jingpin/down.png") top center no-repeat;}
.installBtn{background: url("../images/jingpin/down.png") top center no-repeat;}
.btn .state{display: block; margin:32px 0 0;line-height: 28px; text-align: center;}
.more{display: none;}
.loadMore{display:block;}