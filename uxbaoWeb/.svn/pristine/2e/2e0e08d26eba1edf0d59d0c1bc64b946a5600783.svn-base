    <script type="text/javascript">
        $(document).ready(function () {
            $("#txtPayAccounts").blur(function () {
                if ($(this).val() != "") {
                    setIntegeCheckss(this);
                    if ($(this).hasClass("tooltipinputerr")) {
                        $('#tipTable').css({ left: $.getLeft(this) + 'px', top: $.getTop(this) + 'px' });
                        $('.contenImage').html($(this).attr('hip'));
                        $('#tipTable').fadeIn("fast");
                    }
                } else {
                    $('#tipTable').css({ left: $.getLeft(this) + 'px', top: $.getTop(this) + 'px' });
                    $('.contenImage').html($(this).attr('tip'));
                    $('#tipTable').fadeIn("fast");
                    $(this).removeClass('tooltipinputok').addClass('tooltipinputerr');
                }
            });
            // setUserPaw("txtLogonPass2");
            //确认密码
            $("#txtPayReAccounts").blur(function () {
                if ($(this).val() != "") {
                    Mima(this, $("#txtPayAccounts"), 1);
                    if ($(this).hasClass("tooltipinputerr")) {
                        $('#tipTable').css({ left: $.getLeft(this) + 'px', top: $.getTop(this) + 'px' });
                        $('.contenImage').html($(this).attr('hip'));
                        $('#tipTable').fadeIn("fast");
                    }
                } else {
                    $('#tipTable').css({ left: $.getLeft(this) + 'px', top: $.getTop(this) + 'px' });
                    $('.contenImage').html($(this).attr('tip'));
                    $('#tipTable').fadeIn("fast");
                    $(this).removeClass('tooltipinputok').addClass('tooltipinputerr');
                }
            });
            $("#TextBox1").blur(function () {
                if ($(this).val() != "") {
                    shuzi(this);
                    if ($(this).hasClass("tooltipinputerr")) {
                        $('#tipTable').css({ left: $.getLeft(this) + 'px', top: $.getTop(this) + 'px' });
                        $('.contenImage').html($(this).attr('hip'));
                        $('#tipTable').fadeIn("fast");
                    }
                } else {
                    $('#tipTable').css({ left: $.getLeft(this) + 'px', top: $.getTop(this) + 'px' });
                    $('.contenImage').html($(this).attr('tip'));
                    $('#tipTable').fadeIn("fast");
                    $(this).removeClass('tooltipinputok').addClass('tooltipinputerr');
                }
            })
            function shuzi(dome) {
                if ($(dome).val() != "") {
                    var reg = /^[1-9]\d{0,}$/;
                    if (reg.test($(dome).val())) {
                        $(dome).removeClass('tooltipinputerr').addClass('tooltipinputok');
                        return true;
                    } else {
                        $(dome).removeClass('tooltipinputok').addClass('tooltipinputerr');
                        return false;
                    }
                } else {
                    $(dome).removeClass('tooltipinputok').addClass('tooltipinputerr');
                }
            }
            $('#btnPay').click(function () {
                if (setIntegeCheckss($("#txtPayAccounts")) && Mima($("#txtPayReAccounts"), $("#txtPayAccounts"), 2) && shuzi($("#TextBox1"))) {
                    hiAlert("用户不存在！");
                    return false;
                }
                else {
                    hiAlert("请将信息填写完整！");
                    return false;
                }

            })
        });
    </script>
