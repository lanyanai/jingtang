    <script>
        function worldCupAc()
        {
            $("#kaifu").off("tap").on("tap", function()
            {
                isUxbao && window.uxbao.click(JSON.stringify
                        (
                                {
                                    "type":15,
                                    "title":"世界杯-玩游戏-赢多重好礼",
                                    "url": $.htmlRoot + "world_cup_pre.html"
                                }
                        )
                );
                return false;
            });

            $('#slider').find('img').off('tap').on('tap', function()
            {
                if($(this).data('resName').indexOf('世界杯') !== -1)
                {
                    isUxbao && window.uxbao.click(JSON.stringify
                            (
                                    {
                                        "type":15,
                                        "title":$(this).data('resName'),
                                        "url": $.htmlRoot + "world_cup.html"
                                    }
                            )
                    );
                }
                else
                {
                    isUxbao && window.uxbao.click(JSON.stringify
                            (
                                    {
                                        "type":2,
                                        "url":ajaxRecommend.detailUrl + "?resId=" + $(this).data('id'),
                                        "resId":$(this).data('id'),
                                        "resName":$(this).data('resName')
                                    }
                            )
                    );
                }

                return false;
            });
        }
        $(function()
        {
            setTimeout(worldCupAc, 1000);
        });
    </script>
