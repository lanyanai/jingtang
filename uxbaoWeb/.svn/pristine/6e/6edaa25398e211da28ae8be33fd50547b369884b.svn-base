var myGift = {
    detailUrl:'http://115.29.177.196/礼包详情.html',
    now:new Date().getTime(),
    default_icon:""
};

//点击进入礼包详情
function itemTapHandler($target)
{
    var num = $target.find(".btn").data("num");
    if(num)
    {
        isUxbao && window.uxbao.click(JSON.stringify
            (
                {
                    "type":12,
                    "title":"礼包详情",
                    "url":ajaxGift.detailUrl +  "?acId=" + $target.data("acId") + "&num=" + num
                }
            )
        );
    }
    else
    {
        isUxbao && window.uxbao.click(JSON.stringify
            (
                {
                    "type":12,
                    "title":"礼包详情",
                    "url":ajaxGift.detailUrl +  "?acId=" + $target.data("acId")
                }
            )
        );
    }
}

function fillItem($item, itemData)
{
    $item.data("acId", itemData.acId).attr("id", itemData.acId);
    $item.find(".percent").show();

    $item.find(".giftTit").text("【礼包内容】");
    var usePer = (itemData.localInfo.activity.acUseNum/itemData.localInfo.activity.acTotalNum * 100).toFixed(0);
    var leftPer = 100 - usePer;
    $item.find(".percentLeft").css("width", usePer + '%');
    $item.find(".percentRight").css("width", (100-usePer) + '%');
    $item.find('.giftState').text("剩余" + leftPer + "%").addClass('onState');
    //已过期
    if(myGift.now > itemData.localInfo.activity.acEndTime)
    {
        $item.find('.btn').text("已过期").addClass('gray');
    }
    //正在进行
    else
    {
        $item.find('.btn').text("已领过").addClass('gray').data("num", num);
    }

    $item.find(".giftIcon").find('img').attr("data-pic", itemData.localInfo.activity.acIcon).attr('src', myGift.default_icon);
    $item.find(".giftName").text(itemData.localInfo.activity.acName);

    $item.find(".content").text(itemData.localInfo.activity.acContent);

    $item.find(".num").find('span').text(itemData.giftNo);

    $item.find('.copy').on('tap', function()
    {
        isUxbao && window.activity.copy($item.find(".num").find('span').text());
        return false;
    });

    $item.on("tap", function()
    {
        itemTapHandler($item, itemData.acType);
        return false;
    });
}

$(function()
{
    var $defaultItem = $(".giftItem");
    myGift.default_icon = $defaultItem.eq(0).find("img").attr('src');
    var $container = $("#gift-list-box");
    for(var i = 0; i < myGiftData.length; ++i)
    {
        if($defaultItem[i])
        {
            var $item = $defaultItem.eq(i);
            fillItem($item, myGiftData[i]);
            $item.find("img").imglazyload({"urlName": "data-pic"});
        }
        else
        {
            var $item = createItem(myGiftData[i]);
            $container.append($item);
            $item.find("img").imglazyload({"urlName":"data-pic"});
        }
    }
    for(var j = myGiftData.length; j < $defaultItem.length; ++j)
    {
        $defaultItem.eq(j).remove();
    }
    $(".giftItem").eq(-1).addClass("lastItem");
});
