body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;-webkit-tap-highlight-color: rgba(0,0,0,0);}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
    color:#252525; font-size:14px; line-height:20px; -webkit-user-select:none;
    background-color: #f3f3f3;
}

.infoList{ margin:0 10px;overflow: hidden; }
.infoItem{ width: 100%; padding:5px 0 8px; color: #2f2f2f;
    background: url("../images/zixun/split.png") bottom center no-repeat;
    background-size: 100%;
}
.info{ height: 48px; width:100%; overflow: hidden; position: relative; border-bottom:1px #d3d9d5 dashed; padding-bottom: 10px;}
.infoIcon{ position: absolute; height: 48px; width: 48px; border-radius: 6px; top: 0; left: 0;}
.infoIcon img{ height: 100%; width: 100%;border-radius: 6px;}
.middle{ margin:0 35px 0 60px; height: 100%;}
.infoName{ font-size: 16px;line-height: 26px;color: #353535;height: 26px;overflow: hidden;}
.infoDate{ font-size:12px; line-height: 22px; font-weight: lighter;color: #9d9d9d;}
.arrow{display: block;position: absolute; height: 48px; width: 30px;
    background: url("../images/zhuanti/arraw.png") center no-repeat;top:0;right: 5px;
}
.infoContent{ font-size:14px; line-height: 21px; margin:5px; color: #8e8e8e;height: 84px; overflow: hidden; position: relative;}
.tit{color: #494949;}
.text{margin:0 105px 0 0; height: 100%; overflow: hidden;}
.infoContent img{ display: block; position: absolute; right: 4px; top: 2px;}

.more{height: 40px; width: 100%; overflow: hidden; text-align: center;}

.loading
{
    width: 40px;
    height: 40px;
    margin: 0 auto;
    background: url('../images/jingpin/ui-loading.png') 0 0 no-repeat;
    -webkit-background-size: 40px 40px;
    -webkit-animation-name: ui-loading-anim;
    -webkit-animation-duration: 1s;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-timing-function: step-start;
}

div#rate img {
    height: 23px;
    width: 23px;
}

@-webkit-keyframes ui-loading-anim {
    from {
        -webkit-transform: rotate(0deg);
    }
    8.32% {
        -webkit-transform: rotate(0deg);
    }
    8.33% {
        -webkit-transform: rotate(30deg);
    }
    16.65% {
        -webkit-transform: rotate(30deg);
    }
    16.66% {
        -webkit-transform: rotate(60deg);
    }
    24.99% {
        -webkit-transform: rotate(60deg);
    }
    25% {
        -webkit-transform: rotate(90deg);
    }
    33.32% {
        -webkit-transform: rotate(90deg);
    }
    33.33% {
        -webkit-transform: rotate(120deg);
    }
    41.65% {
        -webkit-transform: rotate(120deg);
    }
    41.66% {
        -webkit-transform: rotate(150deg);
    }
    49.99% {
        -webkit-transform: rotate(150deg);
    }
    50% {
        -webkit-transform: rotate(180deg);
    }
    58.32% {
        -webkit-transform: rotate(180deg);
    }
    58.33% {
        -webkit-transform: rotate(210deg);
    }
    66.65% {
        -webkit-transform: rotate(210deg);
    }
    66.66% {
        -webkit-transform: rotate(240deg);
    }
    74.99% {
        -webkit-transform: rotate(240deg);
    }
    75% {
        -webkit-transform: rotate(270deg);
    }
    83.32% {
        -webkit-transform: rotate(270deg);
    }
    83.33% {
        -webkit-transform: rotate(300deg);
    }
    91.65% {
        -webkit-transform: rotate(300deg);
    }
    91.66% {
        -webkit-transform: rotate(330deg);
    }
    99.99% {
        -webkit-transform: rotate(330deg);
    }
    to {
        -webkit-transform: rotate(360deg);
    }
}