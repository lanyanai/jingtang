<!DOCTYPE html>
<html>
    <head>
        <title>游戏宝专题列表</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
        <meta content="telephone=no" name="format-detection">
        <link type="text/css" rel="stylesheet" href="css/subject.css"/>
        <script type="text/javascript" src="js/zepto.js"></script>
        <script type="text/javascript" src="js/gmu.merge.js"></script>
        <script type="text/javascript" src="js/subject.js"></script>
    </head>
    <body onselectstart="return false">
        <div class="subjectList">
            <ul id="subject-list-box">
                <li class="subject">
                    <div class="subjectItem">
                        <div class="subjectHead">
                            <img src="images/local/default_bg.png" />
                            <div class="subjectTit">
                                <p class="subjectName"></p>
                                <p class="subjectTime"></p>
                            </div>
                        </div>
                        <div class="subjectDes">
                            <p class="description">

                            </p>
                            <span class="arrow"></span>
                        </div>
                    </div>
                </li>
                <li class="subject">
                    <div class="subjectItem">
                        <div class="subjectHead">
                            <img src="images/local/default_bg.png" />
                            <div class="subjectTit">
                                <p class="subjectName"></p>
                                <p class="subjectTime"></p>
                            </div>
                        </div>
                        <div class="subjectDes">
                            <p class="description">

                            </p>
                            <span class="arrow"></span>
                        </div>
                    </div>
                </li>
                <li class="subject">
                    <div class="subjectItem">
                        <div class="subjectHead">
                            <img src="images/local/default_bg.png" />
                            <div class="subjectTit">
                                <p class="subjectName"></p>
                                <p class="subjectTime"></p>
                            </div>
                        </div>
                        <div class="subjectDes">
                            <p class="description">

                            </p>
                            <span class="arrow"></span>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
        <div class="more">
            <div class="loading"></div>
        </div>
    </body>
    <script>
        function worldCupAc()
        {
            $('.subject').off('tap').on('tap', function()
            {
                if($(this).data('name').indexOf('世界杯') !== -1)
                {
                    isUxbao && window.uxbao.click(JSON.stringify
                            (
                                    {
                                        "type":15,
                                        "title":$(this).data('name'),
                                        "url": $.htmlRoot + "world_cup_pre.html"
                                    }
                            )
                    );
                }
                else
                {
                    isUxbao && window.uxbao.click(JSON.stringify
                            (
                                    {
                                        "type":15,
                                        "title":$(this).data('name'),
                                        "url":ajaxSubject.subjectDetailUrl + '?categoryId=' + $(this).data('rescategoryId')
                                    }
                            )
                    );
                }
            });
        }
        $(function()
        {
            setTimeout(worldCupAc, 1000);
        });
    </script>
</html>