.gameIcon{ position: absolute; right: 0; padding: 3px 20px 3px 5px;
    top: 0;
    width: 90%;
    overflow: hidden;
    margin:3px 0;
    height: 50px;

}
.arrow{ position: absolute; top: 0; right: 5px; height: 100%; width: 11px; background: url("../images/local/arrow.png") center no-repeat;}
.gameIconList{ width: 100%; overflow: hidden; height: 100%;}
.gameIconList li{ float: right; width: 60px; position: relative; overflow:hidden; height: 100%;}
.appIcon{ width: 48px; height: 48px; position: absolute; top: 0; right: 0; bottom: 0; left: 0; margin: auto; display: block;}