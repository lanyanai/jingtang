body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif; background-color: #eaeaea;
    color:#252525; font-size:14px; line-height:20px;-webkit-user-select: none;
}


#categoryList{ margin: 10px 10px 0; overflow: hidden;}
.category{ margin-bottom:10px; overflow: hidden; position: relative;
    height: 64px; border-left: 5px solid #f68521; border-bottom: 1px solid #e4e4e4; border-right: 1px solid #e4e4e4;
    background-color: #fff;box-shadow: 0 2px 2px #dadada;
}
.categoryDes{ width: 118px; height: 64px; position: absolute;}
.des{ display: block; padding-left:54px;width: 64px; height: 100%; text-align: center; line-height: 64px;font-size: 18px;}
#xiuxian .categoryDes{background: url("../images/local/xiuxianbg2.png") center no-repeat;}
#xiuxian .des{color: #5b6f2f;}
