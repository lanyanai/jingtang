body,div,span,img,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,input,textarea,select,option,fieldset,legend,p,th,td{margin:0; padding:0;}
fieldset,img {border: 0;}
a{text-decoration:none; color:#252525;-webkit-tap-highlight-color: rgba(0,0,0,0);}
.clear{clear:both;}
ul,li{list-style:none;}
body{ font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
    color:#252525; font-size:14px; line-height:20px; -webkit-user-select:none;
    background-color: #dedede;
}
.giftList{ margin:5px 10px;overflow: hidden; padding-bottom: 10px;}
.giftItem{ width: 100%; background:url("../images/libao/dibian.png") center bottom no-repeat;
    background-size:100% 16px;padding:5px 0 16px; color: #2f2f2f; background-color: #f6f6f6;
    overflow: hidden;
}
.lastItem{ background-image: none; padding-bottom: 5px;box-shadow: 0 3px 3px #a7a7a7;}
.giftInfo{ height: 48px; width:100%; overflow: hidden; position: relative;}
.giftIcon{ position: absolute; height: 100%; width: 48px; border-radius: 6px; top: 0; left: 5px;}
.giftIcon img{ height: 100%; width: 100%;border-radius: 6px;}
.middle{ margin:0 80px 0 60px; height: 100%;}
.giftName{ font-size: 16px; line-height: 26px;color: #353535; height: 26px; overflow: hidden;}
.giftState{ font-size:12px; line-height: 22px; color: #9d9d9d;}
.onState{color: #f49643;}
.btn{ position: absolute; right: 5px; top:5px; padding:0; width: 74px; text-align: center;
    line-height: 30px; font-size: 16px; color: #fff; background-color: #f49643; border-radius: 6px;
    box-shadow: 0 3px 3px #c97c39;
}

.percent{ width: 100px; height: 4px; float: left; padding: 8px 4px;}
.percentLeft{ display: block; float: left; height: 100%; background-color: #f49643; width: 50%;}
.percentRight{ display: block; float: left; height: 100%; background-color: #d6d6d6; width: 50%;}

.giftContent{ font-size:14px; line-height: 24px; margin:5px; color: #8e8e8e;max-height: 48px; overflow: hidden;}
.giftTit{color: #494949;}

.gray{ background-color: #ccc;box-shadow: 0 3px 3px #999;}

.more{height: 40px; width: 100%; overflow: hidden; text-align: center;}

.loading
{
    width: 40px;
    height: 40px;
    margin: 0 auto;
    background: url('../images/jingpin/ui-loading.png') 0 0 no-repeat;
    -webkit-background-size: 40px 40px;
    -webkit-animation-name: ui-loading-anim;
    -webkit-animation-duration: 1s;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-timing-function: step-start;
}

@-webkit-keyframes ui-loading-anim {
    from {
        -webkit-transform: rotate(0deg);
    }
    8.32% {
        -webkit-transform: rotate(0deg);
    }
    8.33% {
        -webkit-transform: rotate(30deg);
    }
    16.65% {
        -webkit-transform: rotate(30deg);
    }
    16.66% {
        -webkit-transform: rotate(60deg);
    }
    24.99% {
        -webkit-transform: rotate(60deg);
    }
    25% {
        -webkit-transform: rotate(90deg);
    }
    33.32% {
        -webkit-transform: rotate(90deg);
    }
    33.33% {
        -webkit-transform: rotate(120deg);
    }
    41.65% {
        -webkit-transform: rotate(120deg);
    }
    41.66% {
        -webkit-transform: rotate(150deg);
    }
    49.99% {
        -webkit-transform: rotate(150deg);
    }
    50% {
        -webkit-transform: rotate(180deg);
    }
    58.32% {
        -webkit-transform: rotate(180deg);
    }
    58.33% {
        -webkit-transform: rotate(210deg);
    }
    66.65% {
        -webkit-transform: rotate(210deg);
    }
    66.66% {
        -webkit-transform: rotate(240deg);
    }
    74.99% {
        -webkit-transform: rotate(240deg);
    }
    75% {
        -webkit-transform: rotate(270deg);
    }
    83.32% {
        -webkit-transform: rotate(270deg);
    }
    83.33% {
        -webkit-transform: rotate(300deg);
    }
    91.65% {
        -webkit-transform: rotate(300deg);
    }
    91.66% {
        -webkit-transform: rotate(330deg);
    }
    99.99% {
        -webkit-transform: rotate(330deg);
    }
    to {
        -webkit-transform: rotate(360deg);
    }
}