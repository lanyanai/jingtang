    <body onselectstart="return false">

        <!--logo与搜索-->
        <div class="logo-header">
            <a class="goback" href="javascript:history.go(-1);"></a>
            <a class="logo" href="http://q.gambao.com">
                <img src="images/local/logo.png" />
            </a>
            <a id="search-btn" href="default_search.html">
                <img src="images/local/search.png" />
            </a>
        </div>

        <!--导航-->
        <div id="nav">
            <ul class="nav-list">
                <li><a href="http://q.gambao.com">首页</a></li>
                <li><a href="ol_category.html">网游</a></li>
                <li><a href="single_category.html">单机</a></li>
                <li><a href="topic_list.html">专题</a></li>
                <li><a href="info_list.html">资讯</a></li>
            </ul>
        </div>
        <!--详情头-->
        <div class="giftHead">
            <div class="giftInfo">
                <div class="giftIcon">
                    <img src="images/local/market.png">
                </div>
                <div class="middle">
                    <p class="giftName"></p>
                    <span class="giftDes"></span>
                </div>
                <div class="btn"></div>
            </div>
            <div class="giftState">
                <p class="state"></p>
                <div class="percent">
                    <span class="percentLeft"></span>
                </div>
            </div>

            <div class="giftNum">
                <p class="num">激活码：<span></span></p>
                <div class="copy">复制</div>
            </div>
        </div>

        <!--礼包内容-->
        <div class="giftContent">
            <div class="header">
                <span class="left"></span>
                <h3 class="title"></h3>
            </div>
            <div class="content">
            </div>
        </div>

        <!--兑换日期-->
        <div class="giftDate">
            <div class="header">
                <span class="left"></span>
                <h3 class="title"></h3>
            </div>
            <div class="date">
            </div>
        </div>

        <!--使用方法-->
        <div class="giftUse">
            <div class="header">
                <span class="left"></span>
                <h3 class="title"></h3>
            </div>
            <div class="steps">
            </div>
        </div>

        <div class="mod-nav-onion">
            <ul>
                <li>
                    <a href="http://q.gambao.com">首页</a>
                </li>
            </ul>
            <a href="#top" class="to-top"><span>回顶部</span></a>
